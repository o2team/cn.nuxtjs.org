{
  "name": "nuxtjs.org",
  "version": "1.0.0",
  "description": "Nuxt.js official website",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.15.2",
    "highlight.js": "^9.8.0",
    "lodash": "^4.17.4",
    "normalize.css": "^5.0.0",
    "nuxt": "latest"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "node-sass": "^4.5.0",
